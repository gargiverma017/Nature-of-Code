let r,g,b,diameter

function setup() {
  createCanvas(windowWidth, windowHeight);
  background(0);
}

function draw() {
  
  r=random(255);
  g=random(255);
  b=random(255);
  diameter=random(20,60)
  fill(r,g,b,100)
  noStroke()
  circle(mouseX,mouseY,diameter)
   
}

function mousePressed(){
  background(0)
}
